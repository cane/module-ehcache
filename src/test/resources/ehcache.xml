<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="5000" timeToLiveSeconds="5000" overflowToDisk="true" />
	<cache name="sampleCache" maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="5" timeToLiveSeconds="0" overflowToDisk="true" />
	<cache name="test.*" maxElementsInMemory="5000" eternal="false"
		timeToIdleSeconds="5" timeToLiveSeconds="0" overflowToDisk="true" />
</ehcache>

